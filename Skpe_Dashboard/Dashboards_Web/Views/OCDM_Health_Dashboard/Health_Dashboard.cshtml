@{
    ViewBag.Title = "OCDM Health Dashboard";
}
<style>
    .box {
        background-color: #8b8386;
        border: 1px solid #0099CC;
        padding: 10px;
        border-radius: 5px;
        font-family: Roboto;
        height: 100px;
        width: 800px;
        color: white;
        margin: 10px
    }

    .mt-2 {
        margin-top: 5px !important;
    }

    .box1 {
        background-color: lightgrey;
        width: 300px;
        border: 15px solid green;
        padding: 50px;
        margin: 20px;
    }

    .divScroll {
        overflow-y: auto;
        background-color: white;
        max-height: 42px;
        text-align: justify;
    }

    .dot {
        height: 15px;
        width: 15px;
        border-radius: 50%;
        display: inline-block;
    }

    .redDot {
        background-color: red;
    }

    .greenDot {
        background-color: green;
    }

    .orangeDot {
        background-color: darkorange;
    }

    .metriceDiv {
        background-color: antiquewhite;
        color: black;
        font-size: 12px;
        font-weight: bold;
        margin: 0px;
    }

    .mainText {
        padding: .25%;
        background-color: #607d8b;
        font-size: 20px;
        color: white;
        font-weight: bold;
    }

    .btnCustom {
        background-color: white;
        border-color: lightslategrey;
    }

    .threadholdTxt {
        font-size: 12px;
        color: white;
        text-align: center;
        margin-left: 2%;
        margin-right: 1%;
    }

    .percentageText {
        font-size: 12px;
        padding-right: 2%;
        color: white;
    }

    .commentSecion {
        font-size: 16px;
        background-color: #4d4a4a;
        text-align: center;
    }

    .rowBorder {
        border: 2px solid #337ab7;
        border-radius: 2px;
        padding: 2px;
        margin-bottom: 10px;
        max-width: 120%;
    }
    .colBorder {
        border: 2px solid #337ab7;
        border-radius: 2px;
    }
    .btn {
        font-size: 12px;
        padding: 5px;
    }
</style>
<meta http-equiv="refresh" content="240">
<meta name="viewport" content="width=device-width, initial-scale=1">
@*<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">*@

<div class="container-fluid">
    <div class="container-fluid" style="padding: 0%;margin-bottom:2%">
        <div class="btn-group btn-group-justified">
            <div class="btn-group" style="background-color: #607d8b; text-align: center;color: white;">
                Please Select Duration
            </div>
            <div class="btn-group">
                <button type="button" class="btn btn-primary" style="background-color: #4d4a4a;" onclick="hourlyData(1);">1 Hour</button>
            </div>
            <div class="btn-group">
                <button type="button" class="btn btn-primary" style="background-color: #4d4a4a;" onclick="hourlyData(6);">6 Hour</button>
            </div>
            <div class="btn-group">
                <button type="button" class="btn btn-primary" style="background-color: #4d4a4a;" onclick="hourlyData(12);">12 Hour</button>
            </div>
            <div class="btn-group">
                <button type="button" class="btn btn-primary" style="background-color: #4d4a4a;" onclick="dayData(1);">1 Day</button>
            </div>
            <div class="btn-group">
                <button type="button" class="btn btn-primary" style="background-color: #4d4a4a;" onclick="dayData(7);">7 Days</button>
            </div>
        </div>
    </div>
    <div class="container-fluid" style="text-align: center;">
        <div class="row">
            <div class="col-sm-6 colBorder" id="mainCTPM" style="display: none;padding:2px;margin-bottom:10px">
                <div class="mainText">CPU Time Per Machine</div>
                <div class="divScroll" id="CTPM" style="display:none">
                </div>
                <div class="commentSecion">
                    <span class="threadholdTxt">Threshold</span>
                    <span class="dot greenDot"></span>
                    <span class="percentageText"> >@Session["CPUThresholdLow"] </span>
                    <span class="dot orangeDot"></span>
                    <span class="percentageText"> >@Session["CPUThresholdLow"] & < @Session["CPUThresholdHigh"] </span>
                    <span class="dot redDot"></span>
                    <span class="percentageText"> >=@Session["CPUThresholdHigh"] (Continuous @Session["CPUTimeSpan"] occurances)</span>
                </div>
            </div>
            <div class="col-sm-6 rowBorder" id="mainMU" style="display: none;padding:2px;margin-bottom:10px">
                <div class="mainText">Memory Usage</div>
                <div class="divScroll" id="MU" style="display:none">
                </div>
                <div class="commentSecion">
                    <span class="threadholdTxt">Threshold</span>
                    <span class="dot greenDot"></span>
                    <span class="percentageText"> >@Session["MemoryThresholdLow"] </span>
                    <span class="dot orangeDot"></span>
                    <span class="percentageText"> >@Session["MemoryThresholdLow"] & < @Session["MemoryThresholdHigh"] </span>
                    <span class="dot redDot"></span>
                    <span class="percentageText"> >=@Session["MemoryThresholdHigh"]</span>
                </div>
            </div>

        </div>
        <div class="row rowBorder" id="mainAdLatency" style="display:none">
            <div class="col-sm-12" style="padding: 0%; margin: 0%;">
                <div class="mainText">AD Save Latency</div>
                <div class="divScroll" id="AdLatency">
                </div>
                <div class="commentSecion">
                    <span class="threadholdTxt">Threshold</span>
                    <span class="dot greenDot"></span>
                    <span class="percentageText"> >@Session["ADSThresholdLow"] </span>
                    <span class="dot orangeDot"></span>
                    <span class="percentageText"> >@Session["ADSThresholdLow"] & < @Session["ADSThresholdHigh"] </span>
                    <span class="dot redDot"></span>
                    <span class="percentageText"> >=@Session["ADSThresholdHigh"] (Continuous @Session["ADSTimeSpan"] occurances) </span>
                </div>
            </div>
        </div>
        <div class="row rowBorder" id="mainTPF" style="display:none">
            <div class="col-sm-12" style="padding: 0%; margin: 0%;">
                <div class="mainText">Tenant Provision Failures</div>
                <div class="divScroll" id="TPF">
                </div>
                <div class="commentSecion">
                    <span class="threadholdTxt">Threshold</span>
                    <span class="dot greenDot"></span>
                    <span class="percentageText"> >@Session["TenantThresholdLow"] </span>
                    <span class="dot orangeDot"></span>
                    <span class="percentageText"> >@Session["TenantThresholdLow"] & < @Session["TenantThresholdHigh"] </span>
                    <span class="dot redDot"></span>
                    <span class="percentageText"> >=@Session["TenantThresholdHigh"] (Continuous @Session["TenantTimeSpan"] occurances)</span>
                </div>
            </div>
        </div>
        <div class="row rowBorder" id="mainTSPF" style="display:none">
            <div class="col-sm-12" style="padding: 0%; margin: 0%;">
                <div class="mainText">Tenant Sub Provision Failures</div>
                <div class="divScroll" id="TSPF">
                </div>
                <div class="commentSecion">
                    <span class="threadholdTxt">Threshold</span>
                    <span class="dot greenDot"></span>
                    <span class="percentageText"> >@Session["TenantThresholdLow"] </span>
                    <span class="dot orangeDot"></span>
                    <span class="percentageText"> >@Session["TenantThresholdLow"] & < @Session["TenantThresholdHigh"] </span>
                    <span class="dot redDot"></span>
                    <span class="percentageText"> >=@Session["TenantThresholdHigh"] (Continuous @Session["TenantTimeSpan"] occurances)</span>
                </div>
            </div>
        </div>
        <div class="row rowBorder" id="mainTPublish" style="display:none">
            <div class="col-sm-12" style="padding: 0%; margin: 0%;">
                <div class="mainText">Tenant/User Publish Failures</div>
                <div class="divScroll" id="TPublish">
                </div>
                <div class="commentSecion">
                    <span class="threadholdTxt">Threshold</span>
                    <span class="dot greenDot"></span>
                    <span class="percentageText"> >@Session["TenantThresholdLow"] </span>
                    <span class="dot orangeDot"></span>
                    <span class="percentageText"> >@Session["TenantThresholdLow"] & < @Session["TenantThresholdHigh"] </span>
                    <span class="dot redDot"></span>
                    <span class="percentageText"> >=@Session["TenantThresholdHigh"] (Continuous @Session["TenantTimeSpan"] occurances)</span>
                </div>
            </div>
        </div>
        <div class="row rowBorder" id="mainUSPF" style="display:none">
            <div class="col-sm-12" style="padding: 0%; margin: 0%;">
                <div class="mainText">User Sub Provision Failures</div>
                <div class="divScroll" id="USPF">
                </div>
                <div class="commentSecion">
                    <span class="threadholdTxt">Threshold</span>
                    <span class="dot greenDot"></span>
                    <span class="percentageText"> >@Session["UserSubProvisionThresholdLow"] </span>
                    <span class="dot orangeDot"></span>
                    <span class="percentageText"> >@Session["UserSubProvisionThresholdLow"] & < @Session["UserSubProvisionThresholdHigh"] </span>
                    <span class="dot redDot"></span>
                    <span class="percentageText"> >=@Session["UserSubProvisionThresholdHigh"] (Continuous @Session["UserSubProvisionTimeSpan"] occurances)</span>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-6 colBorder" id="mainUPF" style="display: none;padding:2px;margin-bottom:10px">
                <div class="mainText">User Provision Failures</div>
                <div class="divScroll" id="UPF">
                </div>
                <div class="commentSecion">
                    <span class="threadholdTxt">Threshold - ((Prov Fail count / (Prov Success count + Prov Fail count)) *100)</span>
                    <span class="dot greenDot"></span>
                    <span class="percentageText"> < @Session["UserProvisionEvalValue"] </span>
                    <span class="dot redDot"></span>
                    <span class="percentageText"> >=@Session["UserProvisionEvalValue"]</span>
                </div>
            </div>
            <div class="col-sm-6 colBorder" id="mainMNCUSF" style="display: none;padding:2px;margin-bottom:10px">
                <div class="mainText">MNC Users Failed</div>
                <div class="divScroll" id="MNCUSF">
                </div>
                <div class="commentSecion">
                    <span class="threadholdTxt">Threshold - (Move Failure / (Move Success + Move Failure) *100)</span>
                    <span class="dot greenDot"></span>
                    <span class="percentageText"> >@Session["MNCThresholdLow"] </span>
                    <span class="dot orangeDot"></span>
                    <span class="percentageText">>=@Session["MNCThresholdLow"] & < @Session["MNCThresholdHigh"] </span>
                    <span class="dot redDot"></span>
                    <span class="percentageText"> >=@Session["MNCThresholdHigh"]</span>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-6 colBorder" id="mainPQ" style="display: none;padding:2px;margin-bottom:10px">
                <div class="mainText">Provisioning Queue</div>
                <div class="divScroll" id="PQ">
                </div>
                <div class="commentSecion">
                    <span class="threadholdTxt">Threshold</span>
                    <span class="dot greenDot"></span>
                    <span class="percentageText">>0 </span>
                    <span class="dot redDot"></span>
                    <span class="percentageText">=0 </span>
                </div>
            </div>
            <div class="col-sm-6 colBorder" id="mainSPQ" style="display: none;padding:2px;margin-bottom:10px">
                <div class="mainText">Sub-Provisioning Queue</div>
                <div class="divScroll" id="SPQ">
                </div>
                <div class="commentSecion">
                    <span class="threadholdTxt">Threshold</span>
                    <span class="dot greenDot"></span>
                    <span class="percentageText">>0 </span>
                    <span class="dot redDot"></span>
                    <span class="percentageText">=0 </span>
                </div>
            </div>
        </div>
        <div class="row rowBorder" id="mainPubQ" style="display:none">
            <div class="col-sm-12" style="padding: 0%; margin: 0%;">
                <div class="mainText">Publishing Queue</div>
                <div class="divScroll" id="PubQ">
                </div>
                <div class="commentSecion">
                    <span class="threadholdTxt">Threshold</span>
                    <span class="dot greenDot"></span>
                    <span class="percentageText">>0 </span>
                    <span class="dot redDot"></span>
                    <span class="percentageText">=0 </span>
                </div>
            </div>
        </div>
    </div>
    <div id="loadimgDivImage" style="display: block; text-align:center">
        <img src="~/Content/Asset/ajax-loader.gif" />
    </div>
</div>

