@{
    ViewBag.Title = "MSODSDashboard";
}

<style>
    .box {
        background-color: #8b8386;
        border: 1px solid #0099CC;
        padding: 10px;
        border-radius: 5px;
        font-family: Roboto;
        height: 100px;
        width: 800px;
        color: white;
        margin: 10px
    }

    .mt-2 {
        margin-top: 5px !important;
    }

    .box1 {
        background-color: lightgrey;
        width: 300px;
        border: 15px solid green;
        padding: 50px;
        margin: 20px;
    }

    .divScroll {
        overflow-y: auto;
        background-color: white;
        max-height: 42px;
        text-align: justify;
    }

    .dot {
        height: 15px;
        width: 15px;
        border-radius: 50%;
        display: inline-block;
        margin-top :4px;
    }

    .redDot {
        background-color: red;
    }

    .greenDot {
        background-color: green;
    }

    .orangeDot {
        background-color: darkorange;
    }

    .metriceDiv {
        background-color: antiquewhite;
        color: black;
        font-size: 12px;
        font-weight: bold;
        margin: 0px;
    }

    .mainText {
        padding: .25%;
        background-color: #607d8b;
        font-size: 20px;
        color: white;
        font-weight: bold;
    }

    .btnCustom {
        background-color: white;
        border-color: lightslategrey;
    }

    .threadholdTxt {
        font-size: 12px;
        color: white;
        text-align: center;
        margin-left: 2%;
        margin-right: 1%;
    }

    .percentageText {
        font-size: 12px;
        padding-right: 2%;
        color: white;
    }

    .commentSecion {
        font-size: 16px;
        background-color: #4d4a4a;
        text-align: center;
    }

    .rowBorder {
        border: 2px solid #337ab7;
        border-radius: 2px;
        padding: 2px;
        margin-bottom: 10px;
        max-width: 120%;
    }

    .colBorder {
        border: 2px solid #337ab7;
        border-radius: 2px;
    }

    .btn {
        font-size: 12px;
        padding: 5px;
    }
    .redDot {
        background-color: red;
    }

    .greenDot {
        background-color: green;
    }

    .orangeDot {
        background-color: darkorange;
    }
</style>
